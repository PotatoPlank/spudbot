{
    "name": "potatoplank/spudbot",
    "description": "A discord bot built on top of DiscordPHP",
    "authors": [
        {
            "name": "PotatoPlank",
            "email": "potatoplank@protonmail.com"
        }
    ],
    "require": {
        "team-reflex/discord-php": "^7.3",
        "nesbot/carbon": "^2.66",
        "doctrine/dbal": "^3.6",
        "vlucas/phpdotenv": "^5.5",
        "twig/twig": "^3.6",
        "sentry/sdk": "^3.5",
        "ext-pcntl": "*"
    },
    "autoload": {
        "psr-4": {
            "Spudbot\\": "src/Spudbot"
        }
    },
    "suggest": {
        "ext-uv": ""
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
